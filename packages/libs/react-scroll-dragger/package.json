{
  "name": "react-scroll-dragger",
  "version": "1.0.6",
  "author": "Lyndon Ohhyung Chun <lyndonoc@gmail.com>",
  "main": "dist/index.js",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "classnames": "^2.2.6",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "react-syntax-highlighter": "^12.2.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "scripts": {
    "start": "node_modules/.bin/react-scripts start",
    "start:demo": "node_modules/.bin/react-scripts start",
    "build": "node_modules/.bin/react-scripts build",
    "build:library": "NODE_ENV=production babel src/lib/scroll-dragger -d ./dist --copy-files --no-copy-ignored --delete-dir-on-start --root-mode upward",
    "prepublish": "npm run build:library",
    "test": "node_modules/.bin/react-scripts test",
    "eject": "node_modules/.bin/react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
