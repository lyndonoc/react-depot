{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    [
      "@babel/plugin-transform-runtime",
      {
        "version": "7.5.0"
      }
    ],
    [
      "transform-react-remove-prop-types",
      {
        "removeImport": true
      }
    ]
  ]
}
